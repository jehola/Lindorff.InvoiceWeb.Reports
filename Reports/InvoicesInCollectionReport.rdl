    <DataSet Name="Invoices">
      <Query>
        <DataSourceName>Navision_ODS_FI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ClientId">
            <Value>=Parameters!ClientId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IsValid">
            <Value>=Parameters!IsValid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportDate">
            <Value>=Parameters!ReportDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @CurrentClient VARCHAR(30) = (SELECT TOP 1 [ClientId] FROM [Company] WHERE [Name] = @ClientId);

SELECT
	cle.[Document No_] AS [Invoice number],
	dbo.GetDocumentType(cle.[Document Type]) AS [Document Type],
	cle.[Posting Date] AS [Posting date],
	cle.[Due Date] AS [Due date],
	cl.[Date] AS [Prevention Date],
	cle.[On Hold] AS [Prevention status],
	cl.[User ID] AS [Status set by user],
	c.[No_] AS [Customer number],
	c.[Name] AS [Customer name],
	dbo.GetCustomerGroup(c.[Customer Legal Type]) AS [Customer group],	
	cle.[Contract Id] AS [Contract number],
	cle.[Global Dimension 1 Code] AS [Dimension 1],
	cle.[Global Dimension 2 Code] AS [Dimension 2],
	SUM(CASE WHEN dcle.[Entry Type] = 1 THEN dcle.[Amount (LCY)] ELSE 0 END) AS [Original amount],
	SUM(dcle.[Amount (LCY)]) AS [Open amount] 
FROM 
	[Cust_ Ledger Entry] AS cle
	INNER JOIN [Detailed Cust_ Ledg_ Entry] AS dcle ON (dcle.ClientID = cle.ClientID AND dcle.[Cust_ Ledger Entry No_] = cle.[Entry No_])
	INNER JOIN Customer AS c ON (c.ClientID = cle.ClientID AND c.[No_] = cle.[Customer No_])
	OUTER APPLY (
	   SELECT	TOP 1 
		dbo.GetUserIDFromComment(cl.[User ID], cl.Comment) AS [User ID], 
		cl.[Date] 
	  FROM	[Comment Line] AS cl 
	WHERE 	cl.ClientID = cle.ClientID
		AND cl.No_ = cle.[Customer No_]
		AND (cl.Comment LIKE CONCAT('Reminder stop%', cle.[Document No_]) OR cl.Comment LIKE CONCAT('On Hold%', cle.[Entry No_], '%'))
             ORDER BY	cl.[Line No_] DESC) AS cl
WHERE 
	(@IsValid &lt;&gt; 0)
	AND cle.ClientID = @CurrentClient
	AND cle.[Document Type] in (2,5)	
	AND cle.[Posting Date] &lt;= @ReportDate
	AND cle.[Open] = 1
	AND cle.[On Hold] IN ('EST', 'MSU')
GROUP BY
	cle.[Document No_],
	cle.[Document Type],
	cle.[Posting Date],
	cle.[Due Date],
	cl.[Date],
	cle.[On Hold],
	cl.[User ID],
	c.[No_],
	c.[Name],
	c.[Customer Legal Type],
	cle.[Contract Id],
	cle.[Global Dimension 1 Code],
	cle.[Global Dimension 2 Code]
ORDER BY 
	cle.[Posting Date]</CommandText>
      </Query>
