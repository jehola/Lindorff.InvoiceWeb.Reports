            <Value>=Parameters!ClientId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT [Name] FROM [Company Information] WHERE [ClientId] =  (SELECT TOP 1 [ClientId] FROM [Company] WHERE [Name] = @ClientId)</CommandText>
      </Query>
