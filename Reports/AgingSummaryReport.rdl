        <CommandText>
DECLARE @CompanyShortId nchar(4) = (SELECT TOP 1 [ClientId] FROM [Company] WHERE [Name] = @ClientId)
----------------------------------

DECLARE @MyTable TABLE(
	EntryNumber INT NOT NULL,
	CustomerNumber NVARCHAR(50) NOT NULL,
	DocumentType INT NOT NULL,
	DocumentNumber NVARCHAR(50) NOT NULL,
	PostingDate DATETIME NOT NULL,
	EarliestPrintingDate DATETIME,
	InvoicingType INT,
	DueDate DATETIME NOT NULL,
	InvoiceClass NVARCHAR(50) NULL,
	DueDays INT NOT NULL,
	OpenAmount DECIMAL(38, 20) NOT NULL
);

IF (@IsValid &lt;&gt; 0)
INSERT INTO @MyTable (
	EntryNumber, 
	CustomerNumber, 
	DocumentType, 
	DocumentNumber, 
	PostingDate, 
	EarliestPrintingDate, 
	InvoicingType, 
	DueDate, 
	InvoiceClass, 
	DueDays, 
	OpenAmount)
SELECT 
	R.EntryNumber
	,R.CustomerNumber
	,R.DocumentType
	,R.DocumentNumber
	,R.PostingDate
	,R.EarliestPrintingDate
	,R.InvoicingType
	,R.DueDate
	,R.InvoiceClass
	,R.DueDays
	,COALESCE (SUM(CASE WHEN dcle.[Posting Date] &lt;= @ReportDate THEN dcle.[Amount (LCY)] ELSE 0 END), 0) AS OpenAmount 
FROM
(
SELECT -- invoices 
	cle.[Entry No_] AS EntryNumber 
	,cle.[Customer No_] AS CustomerNumber 
	,cle.[Document Type] AS DocumentType 
	,cle.[Document No_] AS DocumentNumber 
	,cle.[Posting Date] AS PostingDate 
	,COALESCE(sih.[Earliest Printing Date], @NavisionEmptyDate) AS EarliestPrintingDate
	,sih.[Invoicing Type] AS InvoicingType 
	,cle.[Due Date] AS DueDate 
	,sih.[Invoice Class] AS InvoiceClass 
	,DATEDIFF(day, cle.[Due Date], @ReportDate) AS DueDays
 FROM [Cust_ Ledger Entry] AS cle 
	 LEFT JOIN [Sales Invoice Header] AS sih ON (sih.[No_] = cle.[Document No_] AND sih.[ClientID] = cle.[ClientID])
 WHERE cle.[ClientID] = @CompanyShortId 
	AND cle.[Document Type] = 2
	AND cle.[Posting Date] &lt;= @ReportDate 
	AND sih.[Invoice Class] IN  (@InvoiceClass)
	AND cle.[Global Dimension 1 Code] IN (@Dimension1)
	AND cle.[Global Dimension 2 Code] IN (@Dimension2)
	AND 
		(DATEDIFF(day, cle.[Due Date], @ReportDate) &lt;= 0  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart1 AND @HiddenEnd1)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart2 AND @HiddenEnd2)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart3 AND @HiddenEnd3)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart4 AND @HiddenEnd4)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart5 AND @HiddenEnd5)  
		OR DATEDIFF(day, cle.[Due Date], @ReportDate) &gt; @HiddenStartLast)

UNION ALL SELECT -- credit memo
	cle.[Entry No_] AS EntryNumber 
	,cle.[Customer No_] AS CustomerNumber 
	,cle.[Document Type] AS DocumentType 
	,cle.[Document No_] AS DocumentNumber 
	,cle.[Posting Date] AS PostingDate 
	,@NavisionEmptyDate AS EarliestPrintingDate
	,NULL AS InvoicingType 
	,cle.[Due Date] AS DueDate 
	,scih.[Invoice Class] AS InvoiceClass 
	,DATEDIFF(day, cle.[Due Date], @ReportDate) AS DueDays 
 FROM [Cust_ Ledger Entry] AS cle 
	 LEFT JOIN [Sales Cr_Memo Header] scih ON (scih.[No_] = cle.[Document No_] AND scih.[ClientID] = cle.[ClientID]) 
 WHERE cle.[ClientID] = @CompanyShortId 
	AND cle.[Document Type] = 3
	AND cle.[Posting Date] &lt;= @ReportDate 
	AND scih.[Invoice Class] IN  (@InvoiceClass)
	AND cle.[Global Dimension 1 Code] IN (@Dimension1)
	AND cle.[Global Dimension 2 Code] IN (@Dimension2)
	AND 
		(DATEDIFF(day, cle.[Due Date], @ReportDate) &lt;= 0  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart1 AND @HiddenEnd1)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart2 AND @HiddenEnd2)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart3 AND @HiddenEnd3)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart4 AND @HiddenEnd4)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart5 AND @HiddenEnd5)  
		OR DATEDIFF(day, cle.[Due Date], @ReportDate) &gt; @HiddenStartLast)
	
UNION ALL SELECT -- other documents
	cle.[Entry No_] AS EntryNumber 
	,cle.[Customer No_] AS CustomerNumber 
	,cle.[Document Type] AS DocumentType 
	,cle.[Document No_] AS DocumentNumber 
	,cle.[Posting Date] AS PostingDate 
	,@NavisionEmptyDate AS EarliestPrintingDate
	,NULL AS InvoicingType 
	,cle.[Due Date] AS DueDate 
	,NULL AS InvoiceClass 
	,DATEDIFF(day, cle.[Due Date], @ReportDate) AS DueDays
 FROM [Cust_ Ledger Entry] AS cle 
 WHERE cle.[ClientID] = @CompanyShortId 
	AND cle.[Document Type] NOT IN (2,3)
	AND cle.[Posting Date] &lt;= @ReportDate 
	AND cle.[Global Dimension 1 Code] IN (@Dimension1)
	AND cle.[Global Dimension 2 Code] IN (@Dimension2)
	AND 
		(DATEDIFF(day, cle.[Due Date], @ReportDate) &lt;= 0  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart1 AND @HiddenEnd1)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart2 AND @HiddenEnd2)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart3 AND @HiddenEnd3)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart4 AND @HiddenEnd4)  
		OR (DATEDIFF(day, cle.[Due Date], @ReportDate) BETWEEN @HiddenStart5 AND @HiddenEnd5)  
		OR DATEDIFF(day, cle.[Due Date], @ReportDate) &gt; @HiddenStartLast)
) AS R
INNER JOIN Customer c ON (R.CustomerNumber = c.[No_] AND @CompanyShortId = c.[ClientID])  
INNER JOIN [Detailed Cust_ Ledg_ Entry] AS dcle ON (R.EntryNumber = dcle.[Cust_ Ledger Entry No_] AND @CompanyShortId = dcle.[ClientID])  
WHERE
	(((c.[Customer Legal Type] = @CompanyValue AND @Company IN (@CustomerType))
	OR (c.[Customer Legal Type] = @ConsumerValue AND @Consumer IN (@CustomerType))) 
	OR (c.[Customer Legal Type] IS NULL AND @Empty IN (@CustomerType)))
GROUP BY
	R.EntryNumber
	,R.CustomerNumber
	,R.DocumentType
	,R.DocumentNumber
	,R.PostingDate
	,R.EarliestPrintingDate
	,R.InvoicingType
	,R.DueDate
	,R.InvoiceClass
	,R.DueDays
HAVING
	COALESCE (SUM(CASE WHEN dcle.[Posting Date] &lt;= @ReportDate THEN dcle.[Amount (LCY)] ELSE 0 END), 0) &lt;&gt; 0


SELECT 
	R.ListOrder AS ListOrder
	,R.DocumentType AS DocumentType

	,R.SumNotDue AS SumNotDue
	,R.SumRange1 AS SumRange1
	,R.SumRange2 AS SumRange2
	,R.SumRange3 AS SumRange3
	,R.SumRange4 AS SumRange4
	,R.SumRange5 AS SumRange5
	,R.SumRange6 AS SumRange6
	,R.SumTotalDue AS SumTotalDue
	,R.SumTotalOpenBalance AS SumTotalOpenBalance

	,R.CountNotDue AS CountNotDue
	,R.CountRange1 AS CountRange1
	,R.CountRange2 AS CountRange2
	,R.CountRange3 AS CountRange3
	,R.CountRange4 AS CountRange4
	,R.CountRange5 AS CountRange5
	,R.CountRange6 AS CountRange6
	,R.CountTotalDue AS CountTotalDue
	,R.CountTotalOpenBalance AS CountTotalOpenBalance
FROM
(SELECT  
	1 AS ListOrder
	,dbo.GetReportDocumentType(2) AS DocumentType
	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN R.OpenAmount ELSE 0 END) AS SumNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN R.OpenAmount ELSE 0 END) AS SumRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN R.OpenAmount ELSE 0 END) AS SumRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN R.OpenAmount ELSE 0 END) AS SumRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN R.OpenAmount ELSE 0 END) AS SumRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN R.OpenAmount ELSE 0 END) AS SumRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN R.OpenAmount ELSE 0 END) AS SumRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN R.OpenAmount ELSE 0 END) AS SumTotalDue 
	,SUM(R.OpenAmount) AS SumTotalOpenBalance 

	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN 1 ELSE 0 END) AS CountNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN 1 ELSE 0 END) AS CountRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN 1 ELSE 0 END) AS CountRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN 1 ELSE 0 END) AS CountRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN 1 ELSE 0 END) AS CountRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN 1 ELSE 0 END) AS CountRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN 1 ELSE 0 END) AS CountRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN 1 ELSE 0 END) AS CountTotalDue 
	,SUM(1) AS CountTotalOpenBalance 

FROM @MyTable AS R WHERE R.DocumentType = 2
AND (
	(@DeliveryStatus = @All)
	OR (@DeliveryStatus = @Sent AND (R.EarliestPrintingDate &lt;= @ReportDate OR R.InvoicingType = 4))
	OR (@DeliveryStatus = @Pending AND (R.EarliestPrintingDate &gt; @ReportDate AND R.InvoicingType &lt;&gt; 4))
	)
 
UNION SELECT  
	2 AS ListOrder
	,dbo.GetReportDocumentType(3) as DocumentType
	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN R.OpenAmount ELSE 0 END) AS SumNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN R.OpenAmount ELSE 0 END) AS SumRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN R.OpenAmount ELSE 0 END) AS SumRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN R.OpenAmount ELSE 0 END) AS SumRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN R.OpenAmount ELSE 0 END) AS SumRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN R.OpenAmount ELSE 0 END) AS SumRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN R.OpenAmount ELSE 0 END) AS SumRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN R.OpenAmount ELSE 0 END) AS SumTotalDue 
	,SUM(R.OpenAmount) AS SumTotalOpenBalance 

	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN 1 ELSE 0 END) AS CountNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN 1 ELSE 0 END) AS CountRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN 1 ELSE 0 END) AS CountRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN 1 ELSE 0 END) AS CountRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN 1 ELSE 0 END) AS CountRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN 1 ELSE 0 END) AS CountRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN 1 ELSE 0 END) AS CountRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN 1 ELSE 0 END) AS CountTotalDue 
	,SUM(1) AS CountTotalOpenBalance 

FROM @MyTable AS R WHERE R.DocumentType = 3

UNION SELECT  
	3 AS ListOrder
	,dbo.GetReportDocumentType(5) as DocumentType
	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN R.OpenAmount ELSE 0 END) AS SumNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN R.OpenAmount ELSE 0 END) AS SumRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN R.OpenAmount ELSE 0 END) AS SumRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN R.OpenAmount ELSE 0 END) AS SumRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN R.OpenAmount ELSE 0 END) AS SumRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN R.OpenAmount ELSE 0 END) AS SumRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN R.OpenAmount ELSE 0 END) AS SumRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN R.OpenAmount ELSE 0 END) AS SumTotalDue 
	,SUM(R.OpenAmount) AS SumTotalOpenBalance 

	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN 1 ELSE 0 END) AS CountNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN 1 ELSE 0 END) AS CountRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN 1 ELSE 0 END) AS CountRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN 1 ELSE 0 END) AS CountRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN 1 ELSE 0 END) AS CountRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN 1 ELSE 0 END) AS CountRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN 1 ELSE 0 END) AS CountRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN 1 ELSE 0 END) AS CountTotalDue 
	,SUM(1) AS CountTotalOpenBalance 

FROM @MyTable AS R WHERE R.DocumentType = 5

UNION SELECT  
	4 AS ListOrder
	,dbo.GetReportDocumentType(1) as DocumentType
	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN R.OpenAmount ELSE 0 END) AS SumNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN R.OpenAmount ELSE 0 END) AS SumRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN R.OpenAmount ELSE 0 END) AS SumRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN R.OpenAmount ELSE 0 END) AS SumRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN R.OpenAmount ELSE 0 END) AS SumRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN R.OpenAmount ELSE 0 END) AS SumRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN R.OpenAmount ELSE 0 END) AS SumRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN R.OpenAmount ELSE 0 END) AS SumTotalDue 
	,SUM(R.OpenAmount) AS SumTotalOpenBalance 

	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN 1 ELSE 0 END) AS CountNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN 1 ELSE 0 END) AS CountRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN 1 ELSE 0 END) AS CountRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN 1 ELSE 0 END) AS CountRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN 1 ELSE 0 END) AS CountRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN 1 ELSE 0 END) AS CountRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN 1 ELSE 0 END) AS CountRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN 1 ELSE 0 END) AS CountTotalDue 
	,SUM(1) AS CountTotalOpenBalance 

FROM @MyTable AS R WHERE R.DocumentType = 1

UNION SELECT  
	5 AS ListOrder
	,dbo.GetReportDocumentType(0) as DocumentType
	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN R.OpenAmount ELSE 0 END) AS SumNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN R.OpenAmount ELSE 0 END) AS SumRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN R.OpenAmount ELSE 0 END) AS SumRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN R.OpenAmount ELSE 0 END) AS SumRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN R.OpenAmount ELSE 0 END) AS SumRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN R.OpenAmount ELSE 0 END) AS SumRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN R.OpenAmount ELSE 0 END) AS SumRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN R.OpenAmount ELSE 0 END) AS SumTotalDue 
	,SUM(R.OpenAmount) AS SumTotalOpenBalance 

	,SUM(CASE WHEN (R.DueDays &lt;= 0) THEN 1 ELSE 0 END) AS CountNotDue 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart1 AND @HiddenEnd1) THEN 1 ELSE 0 END) AS CountRange1 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart2 AND @HiddenEnd2) THEN 1 ELSE 0 END) AS CountRange2 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart3 AND @HiddenEnd3) THEN 1 ELSE 0 END) AS CountRange3 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart4 AND @HiddenEnd4) THEN 1 ELSE 0 END) AS CountRange4 
	,SUM(CASE WHEN (R.DueDays BETWEEN @HiddenStart5 AND @HiddenEnd5) THEN 1 ELSE 0 END) AS CountRange5 
	,SUM(CASE WHEN (R.DueDays &gt; @HiddenStartLast) THEN 1 ELSE 0 END) AS CountRange6 
	,SUM(CASE WHEN (R.DueDays &gt; 0) THEN 1 ELSE 0 END) AS CountTotalDue 
	,SUM(1) AS CountTotalOpenBalance 

FROM @MyTable AS R WHERE R.DocumentType NOT IN (1, 2, 3, 5)
) AS R
 WHERE (@IsValid &lt;&gt; 0) 
 ORDER BY R.ListOrder
 
 </CommandText>
